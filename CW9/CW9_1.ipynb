    "   \"source\": [\n",
    "    \"from sklearn.datasets import load_iris\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"from numpy.linalg import norm , inv\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"iris = load_iris()\\n\",\n",
    "    \"X = iris.data\\n\",\n",
    "    \"y = iris.target\\n\",\n",
    "    \"classes = iris.target_names\\n\",\n",
    "    \"\\n\",\n",
    "    \"means = {}\\n\",\n",
    "    \"for i, cls in enumerate(classes):\\n\",\n",
    "    \"    means[cls] = X[y == i].mean(axis=0)\\n\",\n",
    "    \"\\n\",\n",
    "    \"m_setosa = means[\\\"setosa\\\"]\\n\",\n",
    "    \"m_versicolor = means[\\\"versicolor\\\"]\\n\",\n",
    "    \"m_virginica = means[\\\"virginica\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"print(\\\"Class means:\\\")\\n\",\n",
    "    \"print(\\\" setosa: \\\", m_setosa)\\n\",\n",
    "    \"print(\\\" versicolor: \\\", m_versicolor)\\n\",\n",
    "    \"print(\\\" virginica: \\\", m_virginica)\\n\",\n",
    "    \"\\n\",\n",
    "    \"#eculidean distance\\n\",\n",
    "    \"def eculidean_distance(a,b):\\n\",\n",
    "    \"    diff = a-b\\n\",\n",
    "    \"    return np.sqrt(np.sum(diff ** 2))\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Manhattan distance\\n\",\n",
    "    \"def manhattan_distance(a,b):\\n\",\n",
    "    \"    diff = a-b\\n\",\n",
    "    \"    return np.sum(np.abs(diff))\\n\",\n",
    "    \"\\n\",\n",
    "    \"#Chebyshev distance\\n\",\n",
    "    \"def chebyshev_distance(a,b):\\n\",\n",
    "    \"    diff = a-b\\n\",\n",
    "    \"    return np.max(np.abs(diff))\\n\",\n",
    "    \"# Cosine distance\\n\",\n",
    "    \"def cosine_distance(a,b):\\n\",\n",
    "    \"    cosine = np.dot(a, b) / (norm(a) * norm(b))\\n\",\n",
    "    \"    return 1.0-cosine\\n\",\n",
    "    \"#Mahalanobis distance\\n\",\n",
    "    \"def mahalanobis_distance(a, b, data):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    a, b : دو بردار\\n\",\n",
    "    \"    data : ماتریس داده‌ها برای محاسبه کوواریانس\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    diff = a - b\\n\",\n",
    "    \"    cov_matrix = np.cov(data.T)         # کوواریانس\\n\",\n",
    "    \"    inv_cov = inv(cov_matrix)           # معکوس کوواریانس\\n\",\n",
    "    \"    return np.sqrt(np.dot(np.dot(diff.T, inv_cov), diff))\\n\",\n",
    "    \"\\n\",\n",
    "    \"pairs = [\\n\",\n",
    "    \"(\\\"setosa\\\", \\\"versicolor\\\", m_setosa, m_versicolor),\\n\",\n",
    "    \"(\\\"setosa\\\", \\\"virginica\\\", m_setosa, m_virginica),\\n\",\n",
    "    \"(\\\"versicolor\\\", \\\"virginica\\\", m_versicolor, m_virginica),\\n\",\n",
    "    \"]\\n\",\n",
    "    \"for name1,name2,a,b in pairs:\\n\",\n",
    "    \"    print(f\\\"\\\\nDistances between {name1} and {name2}:\\\")\\n\",\n",
    "    \"    print(f\\\" Euclidean:\\\", eculidean_distance(a, b))\\n\",\n",
    "    \"    print(\\\"Manhattan: \\\",manhattan_distance(a,b))\\n\",\n",
    "    \"    print(\\\"Chebyshev: \\\",chebyshev_distance(a,b))\\n\",\n",
    "    \"    print(\\\"Cosine distance:\\\",cosine_distance(a,b))\\n\",\n",
    "    \"    print(\\\"Mahalanobis distance: \\\",mahalanobis_distance(a,b,X))\\n\"\n",
    "   ],\n",
